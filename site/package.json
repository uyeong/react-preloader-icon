    "@types/react-dom": "^16.9.5",
    "@zeit/next-sass": "^1.0.1",
    "http-server": "^0.12.1",
    "next-compose-plugins": "^2.2.0",
    "node-sass": "^7.0.0",
    "prettier": "^1.19.1",
    "tslint": "^6.1.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "3.8.3"
