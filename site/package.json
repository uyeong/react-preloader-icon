{
  "name": "react-preloader-icon-site",
  "version": "0.0.1",
  "scripts": {
    "lint:pt": "prettier --print-width 120 --single-quote --trailing-comma es5 --write \"./**/*.ts*\"",
    "lint:ts": "tslint -c ../tslint.json -p tsconfig.json --exclude \"node_modules/**\" ./**/*.ts{,x}",
